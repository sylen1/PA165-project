<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appenders>
        <console name="console">
            <patternlayout pattern="%d{hh:mm:ss.sss} [%t]  %-5level %logger{36} - %msg%n"/>
        </console>
    </appenders>
    <loggers>
        <root level="warn">
            <appenderref ref="console"/>
        </root>
    </loggers>

</configuration>